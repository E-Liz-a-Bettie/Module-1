# Part 1:

num1 = int(input('Enter integer:\n'))
num2 = int(input('Enter another integer:\n'))
print(num1, '+', num2, 'is', num1 + num2)
print(num1, '-', num2, 'is', num1 - num2)

# Part 2:

num1 = int(input('Enter integer:\n'))
num2 = int(input('Enter another integer:\n'))
print(num1, '*', num2, 'is', num1 * num2)
print(num1, '/', num2, 'is', num1 / num2)

